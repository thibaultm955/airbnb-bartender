<div class="container">
  <h1> Update your Booking </h1>

  <div class="form-small mb-5 ">
    <%= simple_form_for [@bartender, @booking] do|f| %>
      <%= f.input :start_date, as: :date, html5: true %>
      <%= f.input :end_date, as: :date, html5: true %>
      <%= f.submit%>
    <%end%>
  </div>
</div>
